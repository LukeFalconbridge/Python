from random import random
count = 0
a = random()
a = int(a*10)
print(a)

while count < 4:
    b = int(input("please input your guess:"))
    if b == a:
        print("you win")
        break
    if b > a:
        print("too high")
        count = count + 1
    if b < a:
        print("too low")
        count = count + 1
    if count == 3:
        print("you lose")
        break